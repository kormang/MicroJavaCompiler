program ProgramA
	int x, y, z;
	const int pi = 3, e = 2;
	char a, b;
	const char c = 'c';
{

	void main()
	{
		x = 3; y = x; z=0;
		x++;
		
		while (z < y) {
			x = y + pi * (z - e);
			z++;
		}
		read(a);
	
		if(a==c)
			b = c;
		else{
			x = z; x--;
			b = 'x';
		
		}
		
		while(z < e){ print(b); }
		do { print(x); x--; } while(0<=x);
	}
}